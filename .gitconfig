[color]
  ui = auto
[alias]
  st = status
  di = diff
  ci = commit
  br = branch
  llog = log --date=local
[rerere]
  enabled = true
[pull]
  rebase = false
  default = matching
[core]
  autocrlf = false
  excludesfile = ~/.gitignore_global
[push]
  default = matching
[user]
  name = Matt Ross
  signingkey = CB4B9ABDAA216DE0
[fetch]
  prune = true
